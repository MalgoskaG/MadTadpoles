<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.poland.android.madtadpoles">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_tadpoles"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_tadpoles_round"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.poland.android.madtadpoles.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name="com.poland.android.madtadpoles.HelpActivity" />
        <activity android:name="com.poland.android.madtadpoles.InfoActivity"></activity>
    </application>

</manifest>